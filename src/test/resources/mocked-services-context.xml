<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/mvc 
		http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd	
        http://www.springframework.org/schema/beans     
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="userService" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg value="ua.com.itinterview.service.UserService" />
	</bean>

	<bean id="authenticationUtils" class="org.easymock.EasyMock"
		factory-method="createMock">
		<constructor-arg value="ua.com.itinterview.web.security.AuthenticationUtils" />
	</bean>

</beans>